# Clerk Authentication
# Get your keys from: https://dashboard.clerk.com/last-active?path=api-keys
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=YOUR_PUBLISHABLE_KEY
CLERK_SECRET_KEY=YOUR_SECRET_KEY

# Clerk Frontend API URL (optional, usually auto-detected)
NEXT_PUBLIC_CLERK_FRONTEND_API=https://YOUR_CLERK_FRONTEND_API

# Clerk URL Configuration
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/login
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/dashboard/discover
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/dashboard/discover

